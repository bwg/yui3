{
    "treeview": {
        "requires": [
            "base-build",
            "classnamemanager",
            "treeview-templates",
            "treeview-tree",
            "view"
        ],

        "skinnable": true,

        "submodules": {
            "treeview-node": {
                "requires": [
                    "yui-base"
                ]
            },

            "treeview-templates": {
                "requires": [
                    "template-micro"
                ]
            },

            "treeview-tree": {
                "requires": [
                    "base-build",
                    "treeview-node"
                ],

                "plugins": {
                    "treeview-lazy": {
                        "requires": [
                            "base-pluginhost",
                            "plugin",
                            "treeview-tree"
                        ]
                    }
                }
            }
        }
    }
}
